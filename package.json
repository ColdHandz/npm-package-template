{
  "name": "npm-package-templatet",
  "author": "ColdHands",
  "version": "1.0.13",
  "description": "npm-package-template",
  "main": "./dist/package.js",
  "repository": "https://github.com/ColdHandz/npm-package-template.git",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@coldhandz"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transform": {
      ".*\\.(js)$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js}",
      "!**/node_modules/**"
    ]
  },
  "files": ["dist/package.js"],
  "scripts": {
    "start": "webpack --watch --mode=development",
    "build": "webpack --mode=production --verbose",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "jest": "^26.4.2",
    "terser-webpack-plugin": "^4.1.0",
    "ts-loader": "^8.0.2",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
